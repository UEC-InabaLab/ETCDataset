        {
            "turn": 4,
            "role": "listener",
            "emotions": [
                [
                    "該当なし"
                ],
                [
                    "該当なし"
                ],
                [
                    "喜び"
                ]
            ],
            "utterance": "今度スーパーに行ったら見てみます！お腹が空いてきました。",
            "emotion_transcription": "詳しく教えてもらったので、今度お店で見てみようと思いました。"
