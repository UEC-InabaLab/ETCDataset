        {
            "turn": 2,
            "role": "listener",
            "emotions": [
                [
                    "該当なし"
                ],
                [
                    "喜び"
                ],
                [
                    "該当なし"
                ]
            ],
            "utterance": "将来はイラストレーターになられることを望んでいるのですね",
            "emotion_transcription": "将来に向けて頑張っているのがわかります"
        },
        {
            "turn": 3,
            "role": "speaker",
            "emotions": [
                [
                    "該当なし"
                ],
                [
                    "該当なし"
                ],
                [
                    "該当なし"
                ]
            ],
            "utterance": "本人はサッカー選手になりたいそうです。",
            "emotion_transcription": "親の期待とは裏腹な子どもの将来の夢を伝えました。"
        },
        {
            "turn": 3,
            "role": "listener",
            "emotions": [
                [
                    "悲しみ"
                ],
                [
                    "悲しみ"
                ],
                [
                    "該当なし"
                ]
            ],
            "utterance": "失礼しました。ポジションはどこでしょう？あまり詳しくはないので申し訳ないです。",
            "emotion_transcription": "スポーツには疎くて申し訳ない気持ちです。"
        },
        {
            "turn": 4,
            "role": "speaker",
            "emotions": [
                [
                    "該当なし"
                ],
                [
                    "喜び"
                ],
                [
                    "該当なし"
                ]
            ],
            "utterance": "本人希望でキーパーです。めずらしいですよね",
            "emotion_transcription": "サッカーに詳しくない相手でもわかりやすいポジションで良かったと安心しています"
