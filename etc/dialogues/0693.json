    "dialogue": [
        {
            "turn": 1,
            "role": "speaker",
            "emotions": [
                [
                    "怒り"
                ],
                [
                    "怒り"
                ],
                [
                    "怒り"
                ]
            ],
            "utterance": "兄がいくら言っても、給湯器の電源を切り忘れるんです。",
            "emotion_transcription": "怒りつつ、相手の反応を伺っています。"
        },
        {
            "turn": 1,
            "role": "listener",
            "emotions": [
                [
                    "該当なし"
                ],
                [
                    "該当なし"
                ],
                [
                    "該当なし"
                ]
            ],
            "utterance": "うっかりしたお兄さんなんですね。何度も言っても改善されないとぐったりしますよね。",
            "emotion_transcription": "お兄さんとはどんな関係性なのか知りたいと思いました。"
        },
        {
            "turn": 2,
            "role": "speaker",
            "emotions": [
                [
                    "喜び"
                ],
                [
                    "喜び"
                ],
                [
                    "喜び"
                ]
            ],
            "utterance": "本当にウンザリします。朝シャワーを浴びるんですが、時間ギリギリなのでいつも慌てています。",
            "emotion_transcription": "相手の同意に嬉しく思っています。分かりやすく、現状を伝えようとしています。"
        },
        {
            "turn": 2,
            "role": "listener",
            "emotions": [
                [
                    "嫌悪"
                ],
                [
                    "該当なし"
                ],
                [
                    "該当なし"
                ]
            ],
            "utterance": "それはストレスですよね。朝バタバタするのは私も同じなので、お兄さんの気持ちも少しわかるような気がします。",
            "emotion_transcription": "相手の状況を理解しつつ、自分にはお兄さんにも通じるところがあることを伝えました。"
        },
        {
            "turn": 3,
            "role": "speaker",
            "emotions": [
                [
                    "嫌悪"
                ],
                [
                    "該当なし"
                ],
                [
                    "怒り"
                ]
            ],
            "utterance": "寝坊しなければいいだけなんですがね。先に家族が出ると、兄が最後になるんですが、下手すると一日中電源が点けっぱなしになるんで母にも怒られています。",
            "emotion_transcription": "どっちの気持ちもわかるが、相手にはこちら側になってほしい気持ちもあります。"
        },
        {
            "turn": 3,
            "role": "listener",
            "emotions": [
                [
                    "該当なし"
                ],
                [
                    "該当なし"
                ],
                [
                    "該当なし"
                ]
            ],
            "utterance": "慌ただしくそのまま出ちゃうパターンが何回もあるというわけですね。それは何とかしたいですね。",
            "emotion_transcription": "かなり問題になっていそうなので、解決策を探したいと思いました。"
        },
        {
            "turn": 4,
            "role": "speaker",
            "emotions": [
                [
                    "嫌悪"
                ],
                [
                    "該当なし"
                ],
                [
                    "該当なし"
                ]
            ],
            "utterance": "電気代が高騰しているので、そういうのは避けたいんです。あまり言いすぎると喧嘩になったり、母に注意されるので様子見に留まっています。",
            "emotion_transcription": "どうにもできないもどかしさを、共有したいと思っています。"
        },
        {
            "turn": 4,
            "role": "listener",
            "emotions": [
                [
                    "該当なし"
                ],
                [
                    "該当なし"
                ],
                [
                    "該当なし"
                ]
            ],
            "utterance": "家族といえども共同生活ですから、快適な暮らしのためにも協力してもらいたいですよね。とはいえ、喧嘩は避けたいところです。",
            "emotion_transcription": "お兄さんと対立したいわけではなさそうな胸の内に共感しました。"
        },
        {
            "turn": 5,
            "role": "speaker",
            "emotions": [
                [
                    "喜び"
                ],
                [
                    "該当なし"
                ],
                [
                    "喜び"
                ]
            ],
            "utterance": "お気遣いありがとうございます。怒るのにも体力がいるので、やんわりと注意するよう努力しようと思います。",
            "emotion_transcription": "主張を否定しない相手の気遣いに、胸を打たれています。"
        },
        {
            "turn": 5,
            "role": "listener",
            "emotions": [
                [
                    "該当なし"
                ],
                [
                    "該当なし"
                ],
                [
                    "該当なし"
                ]
            ],
            "utterance": "怒るのが疲れるのはよく分かります。お兄さんには何とか気を付けてもらえるよう願っています。",
            "emotion_transcription": "あまり我慢しすぎないでほしいと思いました。"
        }
    ],
    "dialogue_emotion": "怒る",
