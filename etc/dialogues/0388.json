            "emotions": [
                [
                    "該当なし"
                ],
                [
                    "喜び"
                ],
                [
                    "該当なし"
                ]
            ],
            "utterance": "一度だけ行かれたことがあるのですね。どんなアトラクションに乗りましたか？",
