            "emotions": [
                [
                    "該当なし"
                ],
                [
                    "該当なし"
                ],
                [
                    "該当なし"
                ]
            ],
            "utterance": "すごいですね。時間かかりましたよね。",
            "emotion_transcription": "相手がすごい頑張ったんだなという尊敬の気持ち"
        },
        {
            "turn": 2,
            "role": "speaker",
